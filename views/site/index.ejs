<div class="row">
    <div class="col-xs-12">
        <h1 class="text-center">Home page </h1>
        <hr>
    </div>
</div>

<form action="success.html" method="get">
    <div class="form-group">
        <label> Area Categories: </label>
        <select name="areaCategory">
      <option value="S"> State</option>
      <option value="CO">County</option>
      <option value="M"> Greater Metropolitan Area</option>
      <option value="C">City</option>
      <option value="N">Neighborhood</option>
      <option value="Z">Zip Code</option>
    </select>
    </div>

    <div class="form-group">
        <label>Zip Code </label><input type="text" pattern="[0-9]{5}" class="form-control" name="areaCode">
    </div>

    <div class="form-group">
        <label> Search For:</label>
        <select name="indicatorCode">
  <option value="A">All Homes</option>
  <option value="SF"> Single Family Residences</option>
  <option value="C">Condominiums</option>
  <option value="MVSF"> Price per Square Foot</option>
  <option value="1B">1 Bedroom</option>
  <option value="2B">2 Bedroom</option>
  <option value="3B">3 Bedroom</option>
  <option value="4B">4 Bedroom</option>
  <option value="5B">5 Bedrooms or more</option>
  <option value="BT">Bottom Tier</option>
  <option value="MT">Middle Tier</option>
  <option value="TT">Top Tier</option>
<option value="RMP"> Median Rent, Homes Listed for Rent</option>
<option value="RAH">Estimated Rent, All Homes in Region</option>
<option value="RZSF"> Estimated Rent per Square Foot</option>
<option value="PRR">Price-to-Rent Ratio</option>
<option value="MLP">Median List Price</option>
<option value="MPC">Median Price Cut</option>
<option value="SLPR">Ratio of Sale Price to List Price</option>
<option value="SFL">Sold for Loss</option>
<option value="SFG">Sold for Gain</option>
<option value="IV">Increasing Values</option>
<option value="DV">Decreasing Values</option>
<option value="SPY">Turnover in Housing Market, Past 1 Year</option>
<option value="HR">Number of Homes for Rent</option>
<option value="HF">Monthly Foreclosures per 10,000 Homes</option>
<option value="FR">Percentage of Sales that were Foreclosures</option>
</select>
    </div>

    <input class="btn btn-primary" type="submit">
</form>

<h1>Search Results</h1>
<div id="curve_chart" style="width: 900px; height: 500px"></div>

<table id="wholeTable" border="1">
    <tr id='r1'>
        <% parsedMain.dataset.column_names.forEach(function(columnName) { %>
            <td>
                <%= columnName %>
            </td>
            <% }); %>
    </tr>
    <% parsedMain.dataset.data.forEach(function(dataPoint) { %>
        <tr>
            <td>
                <%= dataPoint[0] %>
            </td>
            <td>
                <%= dataPoint[1] %>
            </td>
        </tr>
        <% }); %>
</table>

